#include<iostream>
using namespace std;
int main()
{
    int a,b;
    cout<<"Calculating quotient and remainder"<<endl;
    cout<<"Enter a number: "<<endl;
    cin>>a;
    cout<<"Enter the divisor: "<<endl;
    cin>>b;
    cout<<"The quotient is "<<a/b<<endl;
    cout<<"The remainder is "<<a%b<<endl;
    return 0;
}